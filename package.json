{
  "name": "@ktym4a/slidev-theme-ktym4a",
  "version": "0.1.1",
  "type": "module",
  "description": "ktym4a's theme for Slidev",
  "author": {
    "email": "shoma@ktym4a.me",
    "name": "ktym4a",
    "url": "https://ktym4a.me"
  },
  "license": "MIT",
  "homepage": "https://github.com/ktym4a/slidev-theme-ktym4a",
  "repository": {
